{
  "name": "@drift-labs/icons",
  "version": "1.0.0",
  "description": "Convert Figma icons to React components",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build-icons-script": "tsc ./src/generate.ts --esModuleInterop --skipLibCheck",
    "icons": "node ./src/generate.js",
    "icons:debug": "node --inspect-brk --inspect=2233 ./src/generate.js",
    "clean": "rm -rf dist",
    "clean-icons": "rm -rf src/icons/components/*",
    "build": "rm -rf dist && tsc -v && tsc",
    "build:rollup": "rm -rf dist && tsc -v && rollup -c",
    "build:minify": "node build-browser.js"
  },
  "private": true,
  "dependencies": {
    "react": "19.0.0-rc-02c0e824-20241028"
  },
  "devDependencies": {
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "esbuild": "^0.27.1",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "vm-browserify": "^1.1.2",
    "@rollup/plugin-commonjs": "22.0.2",
    "@rollup/plugin-node-resolve": "14.1.0",
    "@rollup/plugin-typescript": "8.5.0",
    "@svgr/core": "5.5.0",
    "@svgr/plugin-prettier": "5.5.0",
    "@svgr/plugin-svgo": "5.5.0",
    "@types/fs-extra": "9.0.13",
    "@types/node": "17.0.8",
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "axios": "0.24.0",
    "chalk": "4.1.2",
    "dotenv": "10.0.0",
    "figma-api-exporter": "0.0.2",
    "fs-extra": "10.0.0",
    "nodemon": "1.18.4",
    "rollup": "2.79.0",
    "rollup-plugin-dts": "4.2.2",
    "ts-node": "10.4.0",
    "tslib": "2.4.1",
    "typescript": "4.5.4"
  },
  "keywords": [
    "react",
    "figma to react",
    "icons",
    "icon generator"
  ]
}
